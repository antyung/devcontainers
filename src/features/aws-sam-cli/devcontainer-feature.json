{
  "id": "aws-sam-cli",
  "version": "1.0.1",
  "name": "aws-sam-cli",
  "description": "",
  "documentationURL": "https://github.com/antyung/devcontainers/blob/main/src/features/aws-sam-cli/README.md",
  "licenseURL": "https://github.com/antyung/devcontainers/blob/main/LICENSE",
  "containerEnv": {
    "POETRY_NO_INTERACTION": "1",
    "POETRY_VIRTUALENVS_IN_PROJECT": "1",
    "POETRY_VIRTUALENVS_CREATE": "1",
    "POETRY_CACHE_DIR": "/opt/poetry/.cache",
    "POETRY_HOME": "/opt/poetry",
    "PATH": "${POETRY_HOME}/bin:${POETRY_HOME}/venv/bin:${PATH}"
  },
  "installsAfter": [
    "ghcr.io/antyung/devcontainers/common-utils:latest",
    "ghcr.io/antyung/devcontainers/aws-cli:latest"
  ]
}
