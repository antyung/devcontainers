{
  "id": "python",
  "version": "1.0.0",
  "name": "python",
  "description": "",
  "documentationURL": "https://github.com/antyung/devcontainers/blob/main/src/features/python/README.md",
  "licenseURL": "https://github.com/antyung/devcontainers/blob/main/LICENSE",
  "containerEnv": {
    "HOME": "/home/vscode",
    "POETRY_NO_INTERACTION": "1",
    "POETRY_VIRTUALENVS_IN_PROJECT": "1",
    "POETRY_VIRTUALENVS_CREATE": "1",
    "POETRY_CACHE_DIR": "${HOME}/.cache/pypoetry",
    "POETRY_HOME": "/opt/poetry"
  },
  "options": {
    "version": {
        "type": "string",
        "proposals": [
            "3.12.0"
        ],
        "default": "3.12.0",
        "description": "Select or enter an Python version."
    }
  }
}
