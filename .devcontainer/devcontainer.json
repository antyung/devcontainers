{
  "name": "devcontainers",
  "dockerComposeFile": [
    "./compose.yml"
  ],
  "service": "devcontainer-devcontainers",
  "remoteUser": "vscode",
  "shutdownAction": "stopCompose",
  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated.defaultProfile.linux": "zsh"
      },
      "extensions": []
    }
  },
  "workspaceFolder": "/home/vscode/devcontainers",
  "features": {
    "ghcr.io/antyung/devcontainers/common-utils:1": {},
    "ghcr.io/antyung/devcontainers/docker-in-docker:1": {}
  }
}
