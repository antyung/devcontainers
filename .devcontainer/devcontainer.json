{
  "image": "ghcr.io/antyung/devcontainers/ubuntu:jammy",
  "name": "devcontainers",
  "features": {
      "ghcr.io/antyung/devcontainers/common-utils:1": {},
      "ghcr.io/antyung/devcontainers/docker-in-docker:1": {},
      "ghcr.io/antyung/devcontainers/devcontainer-cli:1": {}
  },
  "customizations": {
    "vscode": {
      "settings": { "terminal.integrated.defaultProfile.linux": "zsh" },
    "extensions": []
    }
  },
  "containerUser": "vscode",
  "workspaceMount": "source=${localWorkspaceFolder}/,target=/home/vscode/devcontainers,type=bind,consistency=cached", "workspaceFolder": "/home/vscode",
  "postCreateCommand": "sudo chown -R vscode:vscode /home/vscode/devcontainers"
}
